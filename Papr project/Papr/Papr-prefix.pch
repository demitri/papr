#ifndef Papr_prefix_h
#define Papr_prefix_h

// To enable this, under Build Settings -> Other C Flags, add: "-DDLOG_DEBUG=1" under "debug"
#ifdef DLOG_DEBUG
#   define DLog(fmt, ...) NSLog((@"%s [Line %d] " fmt), __PRETTY_FUNCTION__, __LINE__, ##__VA_ARGS__);
#else
#   define DLog(...)
#endif


#endif /* Papr_prefix_h */
